        $(function () {

            loadvwpart('/viewpart/web/footer', '#myfooter');



        });
    </script>
    <div>
        <div id="fbContainer" class="fbContainer">
            <div class="MinimizedWidgetMiddle__container MinimizedWidgetMiddle__right">

                <button class="MinimizedWidgetMiddle__label" type="button" onclick="feedbck();">
                    <div class="MinimizedWidgetMiddle__text">Feedback</div><div class="MinimizedWidgetMiddle__emotionIcon">
                        <div class="EmotionIconDefault__iconEmotionDefault">
                            <span class="EmotionIconDefault__commentIcon EmotionIconDefault__invert">
                                <i class="ti-comments-smiley"></i>
                            </span>
                            <span class="EmotionIconDefault__expressionIcon EmotionIconDefault__invert"></span>
                        </div>
                    </div>
                </button>

            </div>
        </div>
    </div>
</body>
</html>














