                                    <div class="form-group mb-0">
                                        <div class="input-filter">
                                            <input class="form-control pdicon" type="search" placeholder="filter" value="" id="navfilter" />
                                        </div>
                                    </div>


                                </div>


                                @RenderSection("navigation", required: false)

                            </div>
                        </div>
                    </div>

                </div>
                
                <div id="lnav" class="container-fluid lnav-content inHeight">

                    <div id="tbcontent" class="">

                        <div class="pull-right pointer" onclick="pgIndexToggle();"><i id="pin" class="ti-pin-alt"></i></div>
                        <div id="tbrtpn">
                            <span class="tchdr">
                                <i class="ti-menu-alt va-middle"></i>
                                On this page
                            </span>
                            <div>
                                <ul id="tb-pgindex">
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div id="rtpush" class="rtpush">
                        <div class="">
                            <div class="row">
                                <div class="col-12 pl-10 pr-10 mt-50">
                                    <div class="bannertron banner-wt" id="myndc">
                                        <div id="techhdr" class=" plr-0">
                                            <div class="row">
                                                <div class="col-md-9 col-sm-12 plr-0">

                                                    <div id="title-h1">
                                                        <h1 noindex="1" class="inline-blk mb-1">
                                                            <span id="title">
                                                                @ViewBag.Title
                                                            </span>
                                                        </h1>


                                                    </div>
                                                    <div id="subtitle-h2">
