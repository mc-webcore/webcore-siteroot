            </div>
            <div class="mnuBtnRt mob-show">
                <button id='trigger2' class="mnuBtn-wt" onclick="sideMenu('toggle');">
                    <i class="icon-menu"></i>
                </button>
            </div>

        </div>

        <form id="home" method="post">
            @Html.AntiForgeryToken()


           

            <div id="cmsconfpn" class="vaconfpn animated fadeInDown">
