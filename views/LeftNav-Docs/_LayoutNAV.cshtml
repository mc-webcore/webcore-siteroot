                                                            <span id="subtitle">@ViewBag.subTitle</span>
                                                        </h2>
                                                    </div>
                                                </div>
                                                <!--revisions-->
                                                <div class="col-md-3 col-sm-12 plr-0 mob-hide">

                                                    <div class="row">
                                                        <div class="col-12 plr-0 ">

                                                            <div id="btn-grp" class="btn-group float-md-right float-lg-right float-xl-right">

                                                                <div id="pgindex" class="smlx pr-15 pt-6x">
                                                                    <ul class="wc-dropdown nocaret">
                                                                        <li>
                                                                            <a href="#">
                                                                                <i class="ti-menu-alt va-middle"></i>
                                                                            </a>
                                                                            <ul class="wc-dropdown-menu mnpos">
                                                                                <li>
                                                                                    <div class="pd-15">
                                                                                        <h6 class="bold">Page Index</h6>

                                                                                        <ul id="pgidx" class="pgidx-ul">
                                                                                        </ul>

                                                                                    </div>
                                                                                </li>
                                                                            </ul>
                                                                        </li>
                                                                    </ul>
                                                                </div>



                                                                @if (User.Identity.IsAuthenticated)
                                                                {
                                                                    <div class="smlx pr-15 pt-6x">
                                                                        <ul id="emlpn" class="wc-dropdown nocaret">

                                                                            <li>
                                                                                <a href="#">
                                                                                    <i class="ti-email va-middle"></i>
                                                                                </a>

                                                                                <ul class="wc-dropdown-menu wc-mt-10">
                                                                                    <li>
                                                                                        <div class="pd-15" id="emlsharepn">
                                                                                            <h5>Share Page</h5>

                                                                                            <span class="sml">eMail</span>
                                                                                            <input type="text" class="form-control ctrlthin" name="emlshare" id="emlshare" placeholder="Recipient email" />
                                                                                            <br />
                                                                                            <textarea rows="3" name="eml-comment" id="eml-comment" maxlength="500" placeholder="Add a message" class="form-control smlx" style="height: 76px;"></textarea>
                                                                                            <div class="mt-10">
                                                                                                <div id="eml-msg" class="grn" style="display:none;">Message Sent</div>
                                                                                                <button type="button" role="button" class="btn btn-thin pull-right" onclick="sharepage();">Send</button>
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="pd-15" id="pnloader">

                                                                                            <img src="/img/loader.svg" class="pnloaderImg" />

                                                                                        </div>

                                                                                    </li>

                                                                                </ul>


                                                                            </li>


                                                                        </ul>



                                                                    </div>
                                                                }

                                                                @if (ViewBag.docpath != null && ViewBag.docpath != "")
                                                                {
                                                                    <div id="breadcrumb" class="smlx pr-15 pt-6x">
                                                                        <a href="@ViewBag.docpath" onclick="copyLink('@ViewBag.docpath')" title="Open & Copy Link to Clipboard">
                                                                            <i class="ti-share "></i>
                                                                        </a>
                                                                    </div>
                                                                }




                                                                @RenderSection("Revision", required: false)



                                                            </div>







                                                        </div>
                                                    </div>


                                                </div>

                                            </div>
                                            <div class="row d-none d-sm-none d-md-flex d-lg-flex">
                                                <div class="col-9 pl-0 ">
                                                    <div id="lastupd">


                                                        <div class="infopn">
                                                            <div id="lastupd">



                                                                @RenderSection("LastUpdate", required: false)

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-3 pr-0">
                                                    <div class="row">
                                                        <div class="col-12 plr-0 pt-10">
                                                            @*<div id="pg-feedback-lnk" class="smxb pull-right">
                                                        <a href="#" onclick="feedbck()" title="Give Page Feedback">
                                                            <i class="ti-thumb-up fa-gry"></i> Feedback
                                                        </a>
                                                    </div>*@
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>



                        <div class="content-pad">

                            <div id="rsView">

                                @RenderBody()
                            </div>
                        </div>

                        <div id="pnView" class="overlay content-pad" style="display:none">

                        </div>


                    </div>
                    <!--footer class="sticky-footer2">
                        <div id="myfooter">

                        </div>
                    </footer-->

                    <div class="cmsfooter-wt bg-lgrey text-center">
                        <div>
                            <div>
                                <p>

                                    powered by <i class="icon-hex-logo"></i> webcore cmx

                                </p>
                            </div>
                        </div>

                    </div>


                </div>

                <div class="rt-menu">

                </div>
            </div>
            <input id="pgId" name="pgId" type="hidden" value="@ViewBag.pgId" />
            <input id="uqId" name="uqId" type="hidden" value="@ViewBag.uniqueId" />

            <div id="pnPU" class="overlay animated fadeIn ">
                <div id="pnPUC" class="fbpan animated slideInRight">

                </div>
            </div>


        </form>


    </div>

    <form id="account" method="post" action="/Account/LogOff">
    </form>


    <div id="ldrpan" class="loader-pg hide">

    </div>


    <div id="loadr" class="nldrpn-lftn-pn  animated fadeIn hide">

        <div class="nldrspan-svg">
            <h1 noindex="1">Loading...</h1>
            <svg role="img"
                 width="100%"
                 height="700"
                 aria-labelledby="loading-aria"
                 viewBox="0 0 600 800"
                 preserveAspectRatio="none">
                <title id="loading-aria">Loading...</title>
                <rect x="0"
                      y="0"
                      width="100%"
                      height="100%"
                      clip-path="url(#clip-path)"
                      style='fill: url("#fill");'></rect>
                <defs>
                    <clipPath id="clip-path">
                        <rect x="0" y="53" rx="3" ry="3" width="565" height="9" />
                        <rect x="0" y="68" rx="3" ry="3" width="568" height="9" />
                        <rect x="0" y="88" rx="3" ry="3" width="178" height="10" />
                        <rect x="2" y="107" rx="0" ry="0" width="568" height="110" />
                        <rect x="439" y="115" rx="0" ry="0" width="0" height="1" />
                        <rect x="-12" y="3" rx="0" ry="0" width="471" height="33" />
                        <rect x="0" y="229" rx="3" ry="3" width="568" height="9" />
                        <rect x="-1" y="245" rx="3" ry="3" width="568" height="9" />
                        <rect x="-6" y="265" rx="3" ry="3" width="178" height="11" />
                        <rect x="-2" y="290" rx="3" ry="3" width="574" height="9" />
                        <rect x="-3" y="306" rx="3" ry="3" width="574" height="9" />
                        <rect x="3" y="381" rx="3" ry="3" width="574" height="9" />
                        <rect x="3" y="396" rx="3" ry="3" width="574" height="9" />
                        <rect x="5" y="416" rx="3" ry="3" width="178" height="10" />
                        <rect x="4" y="439" rx="0" ry="0" width="568" height="110" />
                        <rect x="1" y="325" rx="3" ry="3" width="574" height="9" />
                        <rect x="-1" y="344" rx="3" ry="3" width="574" height="9" />
                        <rect x="-1" y="564" rx="3" ry="3" width="574" height="9" />
                        <rect x="-1" y="579" rx="3" ry="3" width="574" height="9" />
                        <rect x="1" y="599" rx="3" ry="3" width="178" height="10" />
                    </clipPath>
                    <linearGradient id="fill">
                        <stop offset="0.599964"
                              stop-color="#f3f3f3"
                              stop-opacity="1">
                            <animate attributeName="offset"
                                     values="-2; -2; 1"
                                     keyTimes="0; 0.25; 1"
                                     dur="2s"
                                     repeatCount="indefinite"></animate>
                        </stop>
                        <stop offset="1.59996"
                              stop-color="#ecebeb"
                              stop-opacity="1">
                            <animate attributeName="offset"
                                     values="-1; -1; 2"
                                     keyTimes="0; 0.25; 1"
                                     dur="2s"
                                     repeatCount="indefinite"></animate>
                        </stop>
                        <stop offset="2.59996"
                              stop-color="#f3f3f3"
                              stop-opacity="1">
                            <animate attributeName="offset"
                                     values="0; 0; 3"
                                     keyTimes="0; 0.25; 1"
                                     dur="2s"
                                     repeatCount="indefinite"></animate>
                        </stop>
                    </linearGradient>
                </defs>
            </svg>

        </div>
    </div>



    <div id="scrollTop" class="scTop" onclick="ScrollTop();">

        <i class="far fa-caret-square-up fa-3x"></i>
    </div>




    @WScripts.Render("{assetpath}LeftNav-Docs/jQuery/jquery-3.6.0.js")
    @WScripts.Render("{assetpath}LeftNav-Docs/magnifyImg/jquery.magnific-popup.js")

    @WScripts.Render("{assetpath}bootstrap/bootstrap.bundle.js")
    @WScripts.Render("{assetpath}LeftNav-Docs/jquery.slidereveal.js")

    @WScripts.Render("{assetpath}LeftNav-Docs/navigation.js")

    @WScripts.Render("{assetpath}LeftNav-Docs/hammer.min.js", "{assetpath}LeftNav-Docs/validation.js")


    @WScripts.Render("{assetpath}LeftNav-Docs/ajxmod.js?v=2", "{assetpath}leftnav-docs/modal/jquery-confirm.js")
    @WScripts.Render("{assetpath}LeftNav-Docs/search.js", "{assetpath}LeftNav-Docs/menu.js")
    @WScripts.Render("{assetpath}LeftNav-Docs/prism/prism.js")
    @WScripts.Render("{assetpath}LeftNav-Docs/snippets.js")

    @WScripts.Render("{assetpath}LeftNav-Docs/wc-feedback.js")
    @WScripts.Render("/js/idserver.js")

    @WStyles.Render("{assetpath}LeftNav-Docs/magnifyImg/magnific-popup.css")
    <script src="~/Scripts/moment.min.js"></script>

    <scriptsection>
        @RenderSection("scripts", required: false)
    </scriptsection>


